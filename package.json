{
  "name": "webda",
  "version": "0.11.0",
  "description": "Expose API with Lambda",
  "keywords": [
    "aws",
    "lamda",
    "api gateway"
  ],
  "author": "Remi Cattiau <remi@cattiau.com>",
  "repository": "git://github.com/loopingz/webda.git",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "pretest": "tsc",
    "test": "nyc mocha --recursive --exit --timeout=100000 -r ts-node/register test/**/*.test.js src/**/*.spec.ts",
    "run": "node core",
    "format": "node ./scripts/format.js",
    "precommit": "npm run test"
  },
  "dependencies": {
    "accept-language": "=3.0.15",
    "ajv": ">=5.2.3",
    "cookie": ">=0.2.3",
    "email-templates": "^2.7.1",
    "fs-finder": "^1.8.1",
    "jsonpath": "^1.0.0",
    "jsonwebtoken": "^8.2.1",
    "mime": ">=2.0.3",
    "mime-types": "^2.1.11",
    "mustache": "^2.2.1",
    "nodemailer": "^4.0.1",
    "nodemailer-ses-transport": "^1.3.0",
    "passport": "=0.4.0",
    "passport-amazon": "^1.0.0",
    "passport-facebook": ">=2.0.0",
    "passport-github2": ">=0.1.9",
    "passport-google-oauth": "^1.0.0",
    "passport-oauth": "^1.0.0",
    "passport-twitter": ">=1.0.3",
    "uri-templates": ">=0.2.0",
    "uuid": "^3.0.0"
  },
  "files": [
    "lib"
  ],
  "devDependencies": {
    "@types/node": "^9.6.2",
    "glob": "^7.1.2",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": ">=4.0.0",
    "mocha-typescript": "^1.1.17",
    "nyc": "^11.6.0",
    "prettier": "^1.16.4",
    "remap-istanbul": "^0.11.0",
    "source-map-support": "^0.5.4",
    "ts-node": "^5.0.1",
    "typescript": ">=3.3.3333"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx",
      ".js"
    ],
    "include": [
      "lib/**/*.js"
    ],
    "reporter": [
      "html",
      "lcov",
      "json"
    ],
    "report-dir": "./reports",
    "all": true
  },
  "homepage": "https://webda.io"
}
