{
	"test.webda.io": {
		"global": {
			"params": {
				"TEST": "Global",
				"region": "us-east-1"
			},
			"services": {
				"Authentication": {
					"successRedirect": "https://shootandprove.loopingz.com/user.html",
					"failureRedirect": "/login-error",
					"providers": {
						"facebook": {},
						"email": {},
						"phone": {},
						"twitter": {},
						"google": {},
						"github": {}
					}
				},
				"dynamoidents": {
					"inject": "user",
					"check": "user",
					"map": {
							"dynamousers": {
								"key": "user",
								"target": "idents",
								"fields": "type",
								"cascade": true
							}
					},
					"expose": {
						"restrict": {
							"create": true,
							"update": true,
							"get": true
						}
					},
					"type": "DynamoStore",
					"table": "webda-test-idents"
				},
				"dynamousers": {
					"expose": {
						"url": "/users",
						"restrict": {
							"authentication": true,
							"create": true
						}
					},
					"type": "DynamoStore",
					"table": "webda-test-users"
				},
				"mongoidents": {
					"inject": "user",
					"check": "user",
					"map": {
							"mongousers": {
								"key": "user",
								"target": "idents",
								"fields": "type",
								"cascade": true
							}
					},
					"expose": {
						"restrict": {
							"create": true,
							"update": true,
							"get": true
						}
					},
					"type": "MongoStore",
					"collection": "idents"
				},
				"mongousers": {
					"expose": {
						"url": "/users",
						"restrict": {
							"authentication": true,
							"create": true
						}
					},
					"type": "MongoStore",
					"collection": "users"
				},
				"idents": {
					"inject": "user",
					"check": "user",
					"map": {
							"users": {
								"key": "user",
								"target": "idents",
								"fields": "type",
								"cascade": true
							}
					},
					"expose": {
						"restrict": {
							"create": true,
							"update": true,
							"get": true
						}
					},
					"type": "FileStore",
					"folder": "./test/data/idents",
					"validator": "IdentValidator"
				},
				"users": {
					"check": "user",
					"expose": {
						"url": "/users",
						"restrict": {
							"authentication": true,
							"create": true
						}
					},
					"type": "FileStore",
					"folder": "./test/data/users",
					"validator": "UserValidator"
				},
				"binary": {
					"type": "FileBinary",
					"folder": "./test/data/binaries",
					"map": {
						"users": ["images"]
					}
				},
				"s3binary": {
					"type": "S3Binary",
					"bucket": "webda-test",
					"map": {
						"users": ["s3images"]
					}
				}
			}
		},
		"/urltemplate/{id}": {
			"method": "GET",
			"executor": "debug",
			"lambda": "plop",
			"params": {
				"TEST_ADD": "Users"
			}
		},
		"/": {
			"method": ["GET", "POST"],
			"executor": "debug",
			"params": {
				"accessKeyId": "LOCAL_ACCESS_KEY",
				"secretAccessKey": "LOCAL_SECRET_KEY"
			}
		},
		"/webda": {
			"method": ["GET","POST","DELETE","PUT"],
			"executor": "lambda",
			"lambda": "arn:aws:lambda:us-west-2:277712386420:function:webda-test",
            "params": {}
		}
	}
}
