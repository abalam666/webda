{
	"test.webda.io": {
		"global": {
			"params": {
				"accessKeyId": "YOUR_ACCESS_KEY",
				"secretAccessKey": "YOUR_SECRET_KEY",
				"TEST": "Global"
			},
			"services": {
				"Authentication": {
					"successRedirect": "https://shootandprove.loopingz.com/user.html",
					"failureRedirect": "/login-error",
					"providers": {
						"facebook": {},
						"email": {},
						"phone": {},
						"twitter": {},
						"google": {},
						"github": {}
					}
				},
				"idents": {
					"inject": "user",
					"check": "user",
					"map": {
							"users": {
								"key": "user",
								"target": "idents",
								"fields": "type",
								"cascade": true
							}
					},
					"expose": {
						"restrict": {
							"create": true,
							"update": true,
							"get": true
						}
					},
					"type": "FileStore",
					"folder": "./test/data",
					"validator": "IdentValidator"
				},
				"users": {
					"check": "user",
					"expose": {
						"url": "/users",
						"restrict": {
							"authentication": true,
							"create": true
						}
					},
					"type": "FileStore",
					"folder": "./test/data/users",
					"validator": "UserValidator"
				}
			}
		},
		"/urltemplate/{id}": {
			"method": "GET",
			"executor": "debug",
			"lambda": "plop",
			"params": {
				"TEST_ADD": "Users",
				"TEST": "Local"
			}
		},
		"/": {
			"method": ["GET", "POST"],
			"executor": "debug",
			"params": {
				"accessKeyId": "LOCAL_ACCESS_KEY",
				"secretAccessKey": "LOCAL_SECRET_KEY"
			}
		}
	}
}
