{
	"test.webda.io": {
		"global": {
			"params": {
				"accessKeyId": "YOUR_ACCESS_KEY",
				"secretAccessKey": "YOUR_SECRET_KEY",
				"TEST": "Global"
			},
			"stores": {
				"idents": {
					"inject": "user",
					"check": "user",
					"map": {
							"users": {
								"key": "user",
								"target": "idents",
								"fields": "type"
							}
					},
					"expose": {
						"restrict": {
							"create": true,
							"update": true,
							"get": true
						}
					},
					"type": "FileStore",
					"folder": "./test/data",
					"validator": "IdentValidator"
				},
				"users": {
					"check": "user",
					"cascade": ["idents"],
					"expose": {
						"url": "/users",
						"restrict": {
							"authentication": true,
							"create": true
						}
					},
					"type": "FileStore",
					"folder": "./test/data/users",
					"validator": "UserValidator"
				}
			}
		},
		"/users/{id}": {
			"method": "GET",
			"lambda": "plop",
			"params": {
				"TEST_ADD": "Users",
				"TEST": "Local"
			}
		},
		"/auth/{provider}": {
			"method": "GET",
			"executor": "passport"
		},
		"/": {
			"method": ["GET", "POST"],
			"executor": "debug",
			"params": {
				"accessKeyId": "LOCAL_ACCESS_KEY",
				"secretAccessKey": "LOCAL_SECRET_KEY"
			}
		}
	}
}
